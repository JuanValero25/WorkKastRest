{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [ true, "spaces", 2 ],
    "trailing-comma": [ false ],
    "no-string-literal": false,
    "ordered-imports": false,
    "object-literal-sort-keys" : false,
    "no-console" : false,
    "quotemark": [false],
    "max-line-length": [false],
    "max-classes-per-file": [false]
  },
  "rulesDirectory": []
}
